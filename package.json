{
  "name": "easy-schema-form",
  "version": "1.0.10",
  "description": "",
  "main": "./lib/index.js",
  "unpkg": "dist/main.js",
  "module": "es/index.js",
  "scripts": {
    "start": "DEMO=true NODE_ENV=development webpack-dev-server --mode development --open",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -p",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "release": "./release.sh",
    "prepublishOnly": "npm run release",
    "test": "echo test",
    "clean:source": "rm -rf es && rm -rf lib && rm -rf dist",
    "copy:source": "mkdir -p es &&  cp -rf ./src/* es && mkdir -p lib && cp -rf ./src/* lib",
    "build:commonjs": "cross-env  BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env NODE_ENV=development babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/main.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/main.min.js",
    "build": "npm run clean:source && npm run copy:source && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      ".//help/test-env.js",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
